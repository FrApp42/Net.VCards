{
  "metadata": [
    {
      "src": [
        {
          "src": "../Net.VCards",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Net.VCards",
      "_appTitle": "docs/getting-started.md",
      "_appLogoPath": "images/logo.png",
      "_enableSearch": true,
      "pdf": false
    }
  },
  "redirect_url": ["/docs/introduction.html"]
}